#import <UIKit/UIKit.h>

@interface UIImage (Ext)

/**
  根据图片绘制图片

 @param color 图片颜色
 @return 纯色图片
 */
+ (UIImage *)imageWithColor:(UIColor *)color;

/**
 裁剪图片（圆形头像）
 
 @param imageName 图片名称
 @param imageSize 裁剪图片大小
 @return 裁剪后的圆形图片
 */
+ (UIImage *)clipImageWithName:(NSString *)imageName size:(CGSize)imageSize;

@end
